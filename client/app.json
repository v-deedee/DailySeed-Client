{
  "expo": {
    "name": "DailySeed",
    "slug": "dailyseed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.deedee1508.DailySeedClient"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo/logo.png",
          "color": "#000000",
          "sounds": [
            "./assets/notification_sound.wav",
            "./assets/notification_sound_other.wav"
          ]
        }
      ]
    ],
    "notification": {
      "icon": "./assets/logo/logo.png",
      "color": "#7f2ee1",
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Colab.notes",
      "iosDisplayInForeground": true
    },
    "extra": {
      "eas": {
        "projectId": "00309b16-fb8d-46f9-8c25-0042159f7948"
      }
    }
  }
}
